                textsi += "["+string(i)+"]"
                textsi += dialog.texts[| i];
                textsi += "#"
            }
        }
        
        draw_text_ext(x,y,"      Dialog  $"+string(dialog.ide)+". "+type+":
        "+textsi,-1,300)
        
        break;
        case "info":
        
        draw_text(x,y,"      Dialog  $"+string(dialog.ide)+". "+type+"
        do: "+dialog.doi+"
        id editor: "+string(dialog.toIde)+"
        yy: "+string(dialog.yy)+"
        script: "+string(dialog.script))
        
