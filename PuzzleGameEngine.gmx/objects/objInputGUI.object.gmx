colorl = c_white

xx4_l = (gui_w/2) - ((gui_w/2)/2) - (gui_w*0.10);
xx4_r =  (gui_w/2) + ((gui_w/2)/2) - (gui_w*0.10);
yy4 = (gui_h/2) + gui_h*0.09;
w4 = gui_w*0.20
