            <kind>1</kind>
            <string>// background orange
draw_set_color(c_orange)
draw_rectangulo(xx,yy,w,h,0)

// caption text
draw_set_color(c_white)
draw_set_halign(fa_center)

draw_text(xx2,yy2,caption)

// text input

draw_set_color(c_white)

draw_rectangulo(xx3,yy3,w3,h3,0)

draw_set_color(c_black)

draw_rectangulo(xx3,yy3,w3,h3,1)

draw_text_ext(xx2,yy3,keyboard_string,-1,w3-10)

// botones

draw_set_color(colorl)
draw_rectangulo(xx4_l,yy4,w4,h4,0)
draw_set_color(c_white)
draw_text(xx4_l+(w4/2),yy4,"Aceptar")

if (alarm[0] &lt; 0)
if (mouse_collision(xx4_l,yy4,w4,h4) == true) {
    colorl = c_lime;
    if (mouse_check_button_pressed(mb_left) == true) {
        // script
        event_user(0)
    }
    if (mouse_check_button(mb_left) == true) {
        colorl = c_olive;
    }
} else colorl = c_green;

draw_set_color(colorr)
draw_rectangulo(xx4_r,yy4,w4,h4,0)
draw_set_color(c_white)
draw_text(xx4_r+(w4/2),yy4,"Cancelar")

if (alarm[0] &lt; 0)
if (mouse_collision(xx4_r,yy4,w4,h4) == true) {
    colorr = c_orange;
    if (mouse_check_button_pressed(mb_left) == true) {
        // script
        event_user(1)
    }
    if (mouse_check_button(mb_left) == true) {
        colorr = c_maroon;
    }
} else colorr = c_red;



</string>
          </argument>
        </arguments>
      </action>
