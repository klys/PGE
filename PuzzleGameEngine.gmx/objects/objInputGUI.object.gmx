        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Aceptar Script

show_debug_message(caption+":"+keyboard_string)

if (obj != noone) {
    if (variable_instance_exists(obj,variable) == true) {
        var valor = keyboard_string;
        
        if (number == true) valor = real(valor);
        
        variable_instance_set(obj, variable, valor);
        
        if (script != noone) {
            script_execute(script,true, obj, variable, valor)
        }   
    }
}
instance_destroy()
</string>
